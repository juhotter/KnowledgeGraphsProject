@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix schema: <http://schema.org/>.
@prefix ex: <http://example.org/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

ex:business
  a sh:NodeShape ;
  sh:targetClass schema:FoodEstablishment ;
  sh:property [
    sh:path rdf:type ;
    sh:minCount 1 ;
  ] ;
  sh:property [
    sh:path schema:identifier ;
    sh:minCount 1 ;
    sh:maxLength 22 ;
  ] ;
  sh:property [
    sh:path schema:name ;
    sh:minCount 1 ;
    sh:datatype xsd:string ;
  ] ;
  sh:property [
    sh:path schema:address ;
    sh:minCount 1 ;
    sh:datatype xsd:string ;
  ] ;
  sh:property [
    sh:path schema:addressLocality ;
    sh:minCount 1 ;
    sh:datatype xsd:string ;
  ] ;
  sh:property [
    sh:path schema:addressRegion ;
    sh:minCount 1 ;
  ] ;
  sh:property [
    sh:path schema:postalCode ;
    sh:minCount 1 ;
    sh:datatype xsd:string ;
  ] ;
  sh:property [
    sh:path schema:latitude ;
    sh:minCount 1 ;
    sh:datatype xsd:double ;
  ] ;
  sh:property [
    sh:path schema:longitude ;
    sh:minCount 1 ;
    sh:datatype xsd:double ;
  ] ;
  sh:property [
    sh:path schema:ratingValue ;
    sh:minCount 1 ;
  ] ;
  sh:property [
    sh:path schema:reviewCount ;
    sh:minCount 1 ;
    sh:datatype xsd:integer ;
  ] ;
  sh:property [
    sh:path schema:is_open ;
    sh:minCount 1 ;
    sh:in (1 0) ;
  ] ;
  sh:property [
    sh:path schema:category ;
    sh:minCount 1 ;
  ] ;
  sh:property [
    sh:path schema:hoursAvailable ;
    sh:minCount 1 ;
  ] ;
  sh:property [
    sh:path schema:menu ;
    sh:minCount 1 ;
  ] .
